<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.breckneck.washapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WashApp">
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.AddNewZoneActivity"
            android:theme="@style/DialogActivities"
            android:label="@string/Chooseanewzone"/>
        <activity
            android:name=".ZoneDetails" />
        <activity
            android:name=".TaskDetails" />
        <activity
            android:name=".presentation.AddNewTaskActivity"
            android:theme="@style/DialogActivities"
            android:label="@string/chooseanewtask"/>
    </application>

</manifest>